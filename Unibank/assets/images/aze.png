    function exchangeCalc(parent) {
        from = parseFloat($(parent).find('input[name="fromcurrVal"]').val());
        fromCurr = $(parent).find('select[name="fromcurr"]').val();
        toCurr = $(parent).find('select[name="tocurr"]').val();
        ret = [];
        if (fromCurr == toCurr)
            return from;

        if (fromCurr == 'AZN') {

            return parseFloat(from) / cashRates[toCurr]['sell'];
        } else {
            tmp = parseFloat(from) * cashRates[fromCurr]['buy'];
            if (toCurr == 'AZN')
                return tmp;

            return tmp / cashRates[toCurr]['sell'];
        }
    }
    function exchangeCalcReverse(parent) {
        to = parseFloat($(parent).find('input[name="tocurrVal"]').val());
        fromCurr = $(parent).find('select[name="fromcurr"]').val();
        toCurr = $(parent).find('select[name="tocurr"]').val();
        ret = [];
        if (fromCurr == 'AZN') {
            if (toCurr == 'AZN')
                return from;

            return parseFloat(to) * cashRates[toCurr]['sell'];
        } else {
            tmp = parseFloat(to) / cashRates[fromCurr]['buy'];
            if (toCurr == 'AZN')
                return tmp;

            return tmp * cashRates[toCurr]['sell'];
        }
    }
